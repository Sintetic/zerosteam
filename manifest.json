{
    "manifest_version": 2,

    "name": "ZeroSteam",
    "version": "0.0.5",

    "icons": {
        "16": "16x16.png",
        "32": "32x32.png",
        "48": "48x48.png",
        "128": "128x128.png"
    },

    "permissions": [
        "http://store.steampowered.com/*"
    ],

    "browser_action": {
        "default_title": "Открыть уведомления",
        "default_icon": "48x48.png",
        "default_popup": "popup.html"
    }
}